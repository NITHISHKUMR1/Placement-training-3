import java.util.*;

public class RemoveDuplicates {
    public static void main(String[] args) {
        int[] arr = {2, 4, 5, 2, 7, 4, 9, 5};

        Set<Integer> set = new LinkedHashSet<>();

        for (int i = 0; i < arr.length; i++) {
            set.add(arr[i]);
        }

        System.out.println("Unique elements:");
        for (int n : set) {
            System.out.print(n + " ");
        }
    }
}
